import math
import numpy as np
def ele(m,n,N):
    ele_Real = math.cos((m*n*2*math.pi)/N)
    ele_Complex = -1*math.sin((m*n*2*math.pi)/N)
    ele = complex(ele_Real,ele_Complex)
    return ele

def gen_fft(N):
    matrix=[]
    for m in range(N):
        row = []
        for n in range(N): 
            row.append(ele(m,n,N))
        matrix.append(row)
    matrix = np.array(matrix)
    return np.round(matrix,decimals=5)
